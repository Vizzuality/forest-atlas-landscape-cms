<% unless @site_page.site.site_settings.analytics_key(@site_page.site_id).value.empty? && ENV.fetch('GLOBAL_GA_KEY', '').empty? %>
    <script>
      (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r;
        i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
          }, i[r].l = 1 * new Date();
        a = s.createElement(o),
          m = s.getElementsByTagName(o)[0];
        a.async = 1;
        a.src = g;
        m.parentNode.insertBefore(a, m)
      })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

      <% unless @site_page.site.site_settings.analytics_key(@site_page.site_id).value.empty? %>
      ga('create', '<%= @site_page.site.site_settings.analytics_key(@site_page.site_id).value %>', 'auto');
      ga('send', 'pageview');
      <% end %>

      <% unless ENV.fetch('GLOBAL_GA_KEY', '').empty? %>
      ga('create', '<%= ENV.fetch('GLOBAL_GA_KEY').empty? %>', 'auto', 'global');
      ga('global.send', 'pageview');
      <% end %>

    </script>
<% end %>
