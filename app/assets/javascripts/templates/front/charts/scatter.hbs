{
  "width": {{{width}}},
  "height": {{{height}}},
  "padding": "strict",
  "data": [{
    "name": "table",
    "values": {{{data}}}
  }],
  "scales": [{
    "name": "x",
    "type": "ordinal",
    "range": "width",
    "domain": {
      "data": "table",
      "field": {{{xColumn}}}
    }
  }, {
    "name": "y",
    "type": "linear",
    "domain": {
      "data": "table",
      "field": {{{yColumn}}}
    },
    "nice": true
  }],
  "axes": [{
    "type": "x",
    "scale": "x",
    "title": {{{xLabel}}},
    "tickSize": 5,
    "properties": {
      "labels": {
        "text": {"template": "\{{datum.data | left:10}}"},
        "align": {"value": "right"},
        "baseline": {"value": "middle"},
        "angle": {"value": -45},
        "fontSize": {"value": 11},
        "dy": {"value": 20}
      },
      "title": {
        "dx": {"value": 150},
        "dy": {"value": 5},
        "baseline": {"value": "top"}
      }
    }
  }, {
      "grid": true,
      "layer": "back",
      "type": "y",
      "title": {{{yLabel}}},
      "scale": "y",
      "tickSize": 30,
      "properties": {
        "grid": {
          "stroke": {"value": "#eeeeee"},
          "strokeWidth": {"value": 2},
          "strokeDash": {"value": [5,5]}
          },
        "ticks": {
          "stroke": {"value": "#eeeeee"},
          "strokeDash": {"value": [5,5]}
          },
        "labels": {
          "text": {"template": "\{{datum.data | number:'.2s' }}"},
          "align": {"value": "left"},
          "baseline": {"value": "bottom"},
          "fontSize": {"value": 11},
          "dy": {"value": -10}
          },
          "title": {
            "dx": {"value": 90},
            "dy": {"value": -15},
            "align": {"value": "right"},
            "baseline": {"value": "top"}
          }
      }
  }],
  "marks": [{
    "type": "symbol",
    "from": {
      "data": "table"
    },
    "properties": {
      "enter": {
        "x": {"scale": "x", "field": {{{xColumn}}}, "offset": 30 },
        "y": {"scale": "y", "field": {{{yColumn}}} },
        "fill": {"value": "#97be32"},
        "size": {"value": 15}
      }
    }
  }]
}
